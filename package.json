{
  "name": "damascus-react",
  "version": "0.0.3",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "./index.ts",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:brandon-schabel/damascus-react.git"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "microbundle-crl --no-compress --format modern,cjs --css-modules false",
    "build-watch": "pnpm run build watch",
    "postbuild": "tsc --emitDeclarationOnly",
    "test": "jest -c ./jest.config.js",
    "test:watch": "jest -c ./jest.config.js --watch",
    "storybook": "start-storybook -p 9009 -s public",
    "prepare-version": "pnpm install && pnpm run build",
    "create-patch": "pnpm run prepare-version && pnpm version patch",
    "create-minor": "pnpm run prepare-version && pnpm version minor",
    "create-major": "pnpm run prepare-version && npm version major",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@storybook/addon-contexts": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addons": "^5.3.21",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.21",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/node": "^14.14.21",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "babel-loader": "8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "jest": "^26.6.3",
    "microbundle-crl": "^0.13.11",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "files": [
    "dist"
  ],
  "resolutions": {
    "@types/react": "^16.14.2"
  }
}
